<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.codingkakapo.forgetcheck">

    <application
        android:name="jp.codingkakapo.forgetcheck.ForgetCheckApplication"
        android:allowBackup="true"
        android:icon="@drawable/odekake_check_app_icon"
        android:label="@string/app_name_jp"
        android:roundIcon="@drawable/odekake_check_app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Forgetcheck">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>